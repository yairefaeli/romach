{
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 4,
  "trailingComma": "all",
  "plugins": ["prettier-plugin-sort-imports"],
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "parser": "typescript",
      "style": "eslint",
      "options": {}
    }
  },
  "importOrderSortSpecifiers": true
}
